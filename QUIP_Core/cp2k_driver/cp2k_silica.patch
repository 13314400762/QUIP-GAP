+     REAL(KIND=dp)                            :: dt, infree, kin, roll_tol, &
+                                                 roll_tol_thrs
+     REAL(KIND=dp), DIMENSION(3)              :: vector_r, vector_v
+     REAL(KIND=dp), DIMENSION(3, 3)           :: pv_kin
