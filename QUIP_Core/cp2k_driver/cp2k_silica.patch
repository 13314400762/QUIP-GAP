+       POINTER                                :: molecule_set
+     TYPE(npt_info_type), POINTER             :: npt(:,:)
+     TYPE(old_variables_type), POINTER        :: old
+     TYPE(particle_list_type), POINTER        :: core_particles, particles, &
+                                                 shell_particles
